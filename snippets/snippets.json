    },
    "subtitle2": {
        "prefix": "@subt2",
        "body": "==== ${1:title} ====",
        "description": "Subtitle2",
        "scope": ""
    },
    "subtitle3": {
        "prefix": "@subt3",
        "body": "===== ${1:title} =====",
